      RABBITMQ_DEFAULT_PASS: mypass
      RABBITMQ_DEFAULT_USER: user
    networks:
      default: null
    ports:
    - mode: ingress
      target: 5672
      published: "5672"
      protocol: tcp
    - mode: ingress
      target: 15672
      published: "15672"
      protocol: tcp
    - mode: ingress
      target: 1883
      published: "1883"
      protocol: tcp
